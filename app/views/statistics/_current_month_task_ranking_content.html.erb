<table class="table table-sm card-text border-bottom">
    <thead>
    <tr>
        <th class="pl-3"><%= t('dashboard.ranking') %></th>
        <th><%= mt(:user, :name_cn) %></th>
        <th><%= t('dashboard.total_hours') %></th>
    </tr>
    </thead>
    <tbody>
    <% @current_month_task_ranking.each do |info| %>
        <tr>
            <th class="pl-3"><%= info[:seq] %></th>
            <td><%= info[:username] %></td>
            <td><%= info[:hours] %> h</td>
        </tr>
    <% end %>
    </tbody>
</table>